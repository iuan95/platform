.super__header(oncopy="return false;")
    block lastsection
        include  ./main-contents/lastsection
    footer.footer#footer
        .container
            .footer__content
                .footer__icons
                    .footer__left__icons
                        +iconas("logo7")
                        +iconas("logofootertext")
                    .footer__right__icons
                        +iconas('whatsapp')
                        +iconas('vk')
                        +iconas('telegram')
                .line__undericons
                .footer__data
                    .footer__info
                        span Разделы
                        ul
                            li О нас
                            li Услуги
                            li Справочник
                            li Загрузки
                            li Техническая поддержка
                            li Телефонный справочник

                    .footer__info
                        span Контакты
                        ul
                            li +7 (4112) 398-000
                            li +7 (4112) 398-233
                            li secretar@yaensb.ru
                    .footer__info
                        span Головной офис
                        ul
                            li  677980, г. Якутск,<br>ул. Октябрьская, дом 22, <br>офис 313
                    .footer__info.ga_area
                        span Обратная связь
                        ul
                            li Нашли ошибку? Сообщите нам!<br>Выделите и нажмите Ctr+Enter
                .line__undericons1
                footer.footer__footer
                    span.footer__footer__left © 2022 ГБУ РС(Я) “РЦИТ”
                    span.footer__footer__right Разработан в
                        +iconas('logopl')




